{
  "editor.renderWhitespace": "boundary",
  "files.encoding": "utf8",
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "_book/**": true,
    ".rpt2_cache/**": true,
    "dist/**": true,
    "node_modules/**": true,
    "typings/**": true
  },
  "typescript.tsdk": "./node_modules/typescript/lib"
}
